{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "assets/*": [
        "./assets/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "screens/*": [
        "./screens/*"
      ],
      "services/*": [
        "./services/*"
      ],
      "types/*": [
        "./types/*"
      ],
      "utils/*": [
        "./utils/*"
      ],
      "constants/*": [
        "./constants/*"
      ],
      "hooks/*": [
        "./hooks/*"
      ],
      "context/*": [
        "./context/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
, "babel.config.js"  ]
}
